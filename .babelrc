{
  "presets": ["@babel/preset-env", "@babel/preset-react"],
  "env": {
      "production": {
          "plugins": ["transform-class-properties","@babel/plugin-proposal-object-rest-spread", "styled-jsx/babel", "@babel/plugin-syntax-class-properties" ]
      },
      "development": {
          "plugins": ["transform-class-properties","@babel/plugin-proposal-object-rest-spread", "styled-jsx/babel", "@babel/plugin-syntax-class-properties"]
      },
      "test": {
          "plugins": ["proposal-object-rest-spread", "styled-jsx/babel-test"]
      }
  }
}
